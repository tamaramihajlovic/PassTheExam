@page
@model RIIEdition.Pages.UserPages.ProfilPageModel
@{
string src1="/AppPictures/no_image.gif";
}

<head>
    <link rel="stylesheet" href="~/css/UserPages/ProfilePage.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css" />
</head>

<body>
    <partial name="_HeaderPartial" />
    <div class="container-fluid profil-page-body" id="profil-page-body">
        <main role="main">
            <!--Audio-->
            <audio id="audio1" controls style="display:none">
                <source src="~/AppSounds/audioclip-1588773425-290.mp4" type="audio/mpeg"> Your browser does not support
                the audio element.
            </audio>
            <audio id="audio2" controls style="display:none">
                <source src="~/AppSounds/audioclip-1588773431-269.mp4" type="audio/mpeg"> Your browser does not support
                the audio element.
            </audio>
            <div class="wrapper">
                <nav class="background-transparent-90" id="sidebar">
                    <!-- Sidebar -->
                    <div class="sidebar-header">
                        <div class="sidebar-user bg-secondary rounded">
                            <!--
                                <img src="~/UserPictures/58570869_1034852310047479_8023942775235936256_n.jpg" class="img-fluid rounded-circle mb-2" alt="@Model.MyUser.Name @Model.MyUser.LastName">
				            	-->
                            <div class="container-fluid">
                                @if(Model.MyUser.PictureFilePath==null)
                                {
                                <img src="@src1" class="img-raised rounded-circle mx-auto img-thumbnail mt-4"
                                    class="img-fluid rounded-circle mb-2"
                                    alt="@Model.MyUser.Name @Model.MyUser.LastName">
                                }
                                else
                                {
                                <img src="@Model.MyUser.PictureFilePath.Substring(Model.MyUser.PictureFilePath.IndexOf('/'))"
                                    class="img-fluid rounded-circle mb-2"
                                    alt="@Model.MyUser.Name @Model.MyUser.LastName">
                                }
                                <!--Zavrsavanje slike naseg u avatara-->
                            </div>
                            <div class="font-weight-bold text-white">@Model.MyUser.Name @Model.MyUser.LastName</div>
                        </div>
                        <ul class="list-unstyled components">
                            <p>Profile navigation</p>
                            <li class="active">
                                <a href="#homeSubmenu" data-toggle="collapse" aria-expanded="false"
                                    class="dropdown-toggle" class="h5">Upravljanje profilom</a>
                                <ul class="collapse list-unstyled" id="homeSubmenu">
                                    <li>
                                        <a href="#" onclick="javascript:showPregledProfila()">Pregled profila</a>
                                    </li>
                                    <li>
                                        <a href="#" onclick="javascript:showIzmenaProfila()">Izmena profila</a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="#pageSubmenu" data-toggle="collapse" aria-expanded="false"
                                    class="dropdown-toggle">Aktivnosti</a>
                                <ul class="collapse list-unstyled" id="pageSubmenu">
                                    <li>
                                        <a href="#" onclick="javascript:showFlashKartice()">Fleš kartice</a>
                                    </li>
                                    <li>
                                        <a href="#" onclick="javascript:showKvizovi()">Kvizovi</a>
                                    </li>
                                    <li>
                                        <!--/UserPages/Calendar-->
                                        <a href="#" onclick="javascript:showKalendar()">Kalendar</a>
                                    </li>
                                </ul>
                            </li>

                            <li>
                                <a href="#" onclick="javascript:showFavorites()">Favoriti</a>
                            </li>

                            <li>
                                <a href="#" onclick="javascript:showGrafikoni()">Grafikoni</a>
                            </li>
                            <li>
                                <a href="#" onclick="javascript:showPodesavanjaProfila()">Podešavanja profila</a>
                            </li>
                        </ul>
                        <form class="dropdown-item border-top" method="post">
                            <button class="nav-link btn btn-link text-white sidebar-btn-logout"
                                asp-page="/UserPages/Logout">
                                Logout
                            </button>
                        </form>
                </nav>
                <!-- Sadrzaj -->
                <div id="content">
                    <nav class="navbar navbar-expand-lg navbar-light bg-light">
                        <div class="container-fluid">

                            <button type="button" id="sidebarCollapse" class="navbar-btn">
                                <span></span>
                                <span></span>
                                <span></span>
                            </button>
                            <ul class="nav navbar-nav ml-auto">
                                <li class="nav-item active" id="lab-pregled-profila" style="display: block;">
                                    <a class="lab-pregled-profila" href="#">Pregled profila</a>
                                </li>
                                <li class="nav-item active" id="lab-izmena-profila" style="display: none;">
                                    <a class="lab-izmena-profila" href="#">Izmena profila</a>
                                </li>
                                <li class="nav-item active" id="lab-fles-kartice-profila" style="display: none;">
                                    <a class="lab-fles-kartice-profila" href="#">Fleš kartice</a>
                                </li>
                                <li class="nav-item active" id="lab-kvizovi-profila" style="display: none;">
                                    <a class="lab-kvizovi-profila" href="#">Kvizovi</a>
                                </li>
                                <li class="nav-item active" id="lab-planiranje-profila" style="display: none;">
                                    <a class="lab-planiranje-profila" href="#">Planiranje</a>
                                </li>
                                <li class="nav-item active" id="lab-favorites-profila" style="display: none;">
                                    <a class="lab-favorites-profila" href="#">Favorites</a>
                                </li>
                                <li class="nav-item active" id="lab-grafikoni-profila" style="display: none;">
                                    <a class="lab-grafikoni-profila" href="#">Grafikoni</a>
                                </li>
                                <li class="nav-item active" id="lab-podesavanje-profila" style="display: none;">
                                    <a class="lab-podesavanje-profila" href="#">Podešavanje profila</a>
                                </li>
                            </ul>
                            <button id="button-with-animation" class="btn btn-dark d-inline-block d-lg-none ml-auto"
                                type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                                aria-controls="navbarSupportedContent" aria-expanded="false"
                                aria-label="Toggle navigation">
                                <i class="fas fa-align-justify"></i>
                            </button>

                            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                                <ul class="nav navbar-nav ml-auto">
                                    <li class="nav-item active" id="liPrikaziSakrijProfilnuSliku"
                                        style="display: block;">
                                        <a class="nav-link" href="#"
                                            onclick="javascript:showProfilnaSlikaPregled()">Prikazi/Sakrij: Profilnu
                                            sliku i bedzeve</a>
                                    </li>
                                    <li class="nav-item active" id="liStatistikuKvizova" style="display: none;">
                                        <a class="nav-link" href="#"
                                            onclick="javascript:showStatistikuKvizova()">Prikazi/Sakrij: Statistiku
                                            kvizova</a>
                                    </li>
                                    <li class="nav-item active" id="liStatistikuFlesKartica" style="display: none;">
                                        <a class="nav-link" href="#"
                                            onclick="javascript:showStatistikuFlesKartica()">Prikazi/Sakrij: Statistiku
                                            fleš kartica</a>
                                    </li>
                                    <li class="nav-item active" id="liTogglePodesavanjeBojeGrafovaContainer"
                                        style="display: none;">
                                        <a class="nav-link" href="#"
                                            onclick="javascript:togglePodesavanjeBojeGrafovaContainer()">Prikazi/Sakrij:
                                            Detaljna podešavanja boja grafova</a>
