                                </ul>
                            </div>
                        </div>
                    </nav>
                    <div id="starterDiv" class="container mt-5">
                        <div class="container">
                            <div class="container-fluid bg-white rounded">
                                <div class="col-sm-12 text-center">
                                    <h1 id="dobrodosli" class="bg-white text-secondary"
                                        style="font-family:Book Antiqua;">
                                        <div class="col-sm-12" style="visibility: hidden;">
                                            Another hidden element :3
                                        </div>
                                        Dobrodošli: <span class="text-primary">@Model.MyUser.UserName</span>
                                    </h1>
                                </div>
                                <div class="col-sm-12 text-center" style="">
                                    <img class="img-fluid" id="welcomeImg" style=""
                                        src="~/AppPictures/RIIEditionLogo.jpg" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="div-pregled-profila">
                        <div class="mt-2  rounded col-md p-3">
                            <div id="div-profilna-slika-pregled" class="container p-2 rounded mb-4"
                                style="display: block;">
                                <div class="row">
                                    <div class="col-sm-2">

                                    </div>
                                    <div class="col-sm-3">
                                        <div class="container-fluid text-center">
                                            @if(Model.MyUser.PictureFilePath==null)
                                            {
                                            <img src="@src1"
                                                class="img-raised rounded-circle mx-auto img-thumbnail mt-4"
                                                class="img-fluid rounded-circle mb-2"
                                                alt="@Model.MyUser.Name @Model.MyUser.LastName">
                                            }
                                            else
                                            {
                                            <img src="@Model.MyUser.PictureFilePath.Substring(Model.MyUser.PictureFilePath.IndexOf('/'))"
                                                class="img-raised rounded-circle mx-auto img-thumbnail mt-4"
                                                class="img-fluid rounded-circle mb-2"
                                                alt="@Model.MyUser.Name @Model.MyUser.LastName">
                                            }
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="div-sm-12" style="visibility: hidden;">Lets see if anyone ever finds
                                            about this hidden text :3</div>
                                    </div>
                                    <div class="col-sm-5 bg-white rounded text-center text-secondary" id="bedzeviId">
                                        @if(await Model.isAdmin(Model.MyUser))
                                        {
                                        <div clas="col-sm-12 p-2">
                                            <span style="display: block;" class="admin mt-2 mb-2">Uloga: admin</span>
                                        </div>
                                        }
                                        @if(await Model.isProfessor(Model.MyUser))
                                        {
                                        <div clas="col-sm-12 p-2">
                                            <span style="display: block;" class="professor mt-2 mb-2">Uloga:
                                                profesor</span>
                                        </div>
                                        }else{
                                        <div clas="col-sm-12 p-2">
                                            <span style="display: block;" class="user mt-2 mb-2">Uloga: regularni
                                                član</span>
                                        </div>
                                        }
                                        <div class="row">
                                            <div class="col-sm-2">

                                            </div>
                                            <div class="col-sm-8" id="bedgeId">
                                                <img id=bedgeImgId src="/AppPictures/Bedzevi/sampion.jpg"
                                                    class="img-fluid" alt="Responsive image">
                                            </div>
                                            <div class="col-sm-2">

                                            </div>
                                        </div>
                                        <div class="col-sm-12 text-center" id="brojKvizova"></div>
                                        <div class="col-sm-12 text-center text-danger" id="rank" style="display: none;">
                                            Hall of fame ranking: 1 🥇</div>
                                    </div>
                                </div>
                            </div>
                            <div asp-validation-summary="All" class="text-danger"></div>
                            <form enctype="multipart/form-data">

                                <div class="form-row p-2">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-sm">
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-sm-2 mt-2">
                                                            <label class="text-white">Ime: </label>
                                                        </div>
                                                        <div class="col-sm-10">
                                                            <label class="form-control text-center"
                                                                placeholder="Ime...">@Model.MyUser.Name</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm">
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-sm-2 mt-2">
                                                            <label class="text-white">Prezime: </label>
                                                        </div>
                                                        <div class="col-sm-10">
                                                            <label class="form-control text-center"
                                                                placeholder="Prezime...">@Model.MyUser.LastName</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="container p-2">
                                    <div class="row">
                                        <div class="col-sm-3 mt-2">
                                            <label class="text-white ml-2">Koriničko ime: </label>
                                        </div>
                                        <div class="col-sm-9">
                                            <label class="form-control text-center"
                                                placeholder="Korisničko ime...">@Model.MyUser.UserName</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="container p-2">
                                    <div class="row">
                                        <div class="col-sm-3 mt-2">
                                            <label class="text-white ml-2">Email: </label>
                                        </div>
                                        <div class="col-sm-9">
                                            <label class="form-control text-center"
                                                placeholder="Email...">@Model.MyUser.Email</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="container p-2">
                                    <div class="row">
                                        <div class="col-sm-3 mt-2">
                                            <label class="text-white ml-2">Godina rođenja: </label>
                                        </div>
                                        <div class="col-sm-9">
                                            <label class="form-control text-center mr-2"
                                                placeholder="Godina rođenja...">@Model.MyUser.YearOfBirth</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-row p-2">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-sm">
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-sm-2 mt-2">
                                                            <label class="text-white">Pol: </label>
                                                        </div>
                                                        <div class="col-sm-10">
                                                            @if(Model.MyUser.Gender == 'M')
                                                            {
                                                            <label class="form-control text-center"
                                                                placeholder="Pol...">Muški</label>
                                                            }
                                                            else
                                                            {
                                                            <label class="form-control text-center"
                                                                placeholder="Pol...">Ženski</label>
                                                            }
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm">
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-sm-2 mt-2">
                                                            <label class="text-white">Grad: </label>
                                                        </div>
                                                        <div class="col-sm-10">
                                                            <label class="form-control text-center"
                                                                placeholder="Grad...">@Model.MyUser.City</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="container p-2">
                                    <div class="row">
                                        <div class="col-sm">
                                            <div class="container">
                                                <div class="row">
                                                    <div class="col-sm-2 mt-2">
                                                        <label class="text-white">Indeks: </label>
                                                    </div>
                                                    <div class="col-sm-10">
                                                        <label class="form-control text-center"
                                                            placeholder="Index...">@Model.MyUser.Index</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm">
                                            <div class="container">
                                                <div class="row">
                                                    <div class="col-sm-2">
                                                        <label class="text-white">Godina studija: </label>
                                                    </div>
                                                    <div class="col-sm-10">
                                                        <label class="form-control text-center"
                                                            placeholder="Godina studija...">@Model.MyUser.YearOfStudy</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </form>
                        </div>
                    </div>

                    <!--Izmena profila-->
                    <!--here-->
                    <div id="div-izmena-profila" style="display: none;">
                        <div class="container bg-white mt-2  rounded col-md p-3 text-center">
                            Ovde možete izmeniti Vaš korisnički profil po želji.
                        </div>
                        <div class="mt-2  rounded col-md boja p-3">
                            <div asp-validation-summary="All" class="text-danger"></div>
                            <form enctype="multipart/form-data" method="post">
                                <div class="form-row">
                                    <div class="form-group col">
                                        <input asp-for="@Model.MyUser.Name" class="form-control  "
                                            placeholder="Ime..." />
                                        <span asp-validation-for="@Model.MyUser.Name" class="text-danger "></span>
                                    </div>
                                    <div class="form-group col">
                                        <input asp-for="@Model.MyUser.LastName" class="form-control "
                                            placeholder="Prezime..." />
                                        <span asp-validation-for="@Model.MyUser.LastName" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label asp-for="@Model.MyUser.YearOfBirth" class="text-white"></label>
                                    <input asp-for="@Model.MyUser.YearOfBirth" class="form-control "
                                        placeholder="Godina rodjenja..." />
                                    <span asp-validation-for="@Model.MyUser.YearOfBirth" class="text-danger"></span>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col mt-2">
                                        <span class="text-white">Pol:</span>
                                        <label class="ml-3 mr-3 text-white">Muški</label>
                                        <input type="radio" asp-for="@Model.MyUser.Gender" value="M" checked />
                                        <label class="ml-3 mr-3 text-white">Ženski</label>
                                        <input type="radio" asp-for="@Model.MyUser.Gender" value="Z" />
                                    </div>
                                    <div class="form-group col">
                                        <input asp-for="@Model.MyUser.City" class="form-control "
                                            placeholder="Grad..." />
                                        <span asp-validation-for="@Model.MyUser.City" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col">
                                        <label asp-for="@Model.MyUser.Index" class="text-white"></label>
                                        <input asp-for="@Model.MyUser.Index" class="form-control "
                                            placeholder="Index..." />
                                        <span asp-validation-for="@Model.MyUser.Index" class="text-danger"></span>
                                    </div>
                                    <div class="form-group col">
                                        <label asp-for="@Model.MyUser.YearOfStudy" class="text-white"></label>
                                        <input asp-for="@Model.MyUser.YearOfStudy" class="form-control "
                                            placeholder="Godina studiranja..." />
                                        <span asp-validation-for="@Model.MyUser.YearOfStudy" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col">
                                        <input asp-for="@Model.MyUser.Email" class="form-control "
                                            placeholder="Email..." />
                                        <span asp-validation-for="@Model.MyUser.Email" class="text-danger"></span>
                                    </div>
                                    <div class="form-group col">
                                        <input asp-for="@Model.MyUser.UserName" class="form-control "
                                            placeholder="Korisnicko ime..." />
                                        <span asp-validation-for="@Model.MyUser.UserName" class="text-danger "></span>
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <div class="custom-file ">
                                        <input asp-for="Photo" class="form-control  custom-file-input " />
                                        <label asp-for="Photo" class="custom-file-label ">Izaberite sliku...</label>
                                    </div>
                                </div>
                                <div>
                                    <button type="submit" class="btn btn-secondary">Sacuvaj izmene</button>
                                </div>
                            </form>
                        </div>
                    </div>
                    <!---->
                    <!--Activnosti-->
                    <!--flash kartice-->
                    <div id="div-flash-kartice" style="display: none;">
                        <div class="container bg-white mt-2 rounded col-md p-3 text-center">
                            Ovde su prikazane sve fleš kartice koje ste dosad odigrali.
                        </div>
                        <div class="rounded">
                            <div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <input style="visibility: hidden;" />
                                    </div>
                                </div>
                            </div>
                            <div class="container-fluid bg-white rounded mt-5">
                                <div class="col-sm-12">
                                    <br>
                                    <table id="tableF" class="table table-striped table-hover table-bordered">
                                        <thead id="tableFHead">

                                        </thead>
                                        <tbody id="tableFBody">

                                        </tbody>
                                    </table>
                                    <ul id="brojOsvojenihPoenaFKId">
                                    </ul>
                                    <ul id="datumiFKId">
                                    </ul>
                                    <ul id="predmetiFKId">
                                    </ul>
                                    @foreach(var item in Model.FlashCardData) {
                                    <script>
                                        var li = document.createElement("li");
                                        var ul = document.getElementById("brojOsvojenihPoenaFKId");
                                        li.innerHTML = "@item.Opis";
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                        li = document.createElement("li");
                                        ul = document.getElementById("datumiFKId");
                                        li.innerHTML = "@item.DatumZavrsetkaFK";
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                        li = document.createElement("li");
                                        ul = document.getElementById("predmetiFKId");
                                        li.innerHTML = "@item.NazivPredmeta";
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                    </script>
                                    }
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--Kvizovi-->
                    <div id="div-kvizovi" style="display: none;">
                        <div class="container bg-white mt-2  rounded col-md p-3 text-center">
                            <div class="col-sm-12">
                                Ovde su prikazani svi kvizovi koje ste dosad odigrali.
                            </div>
                            <div class="col-sm-12">
                                Ukoliko želite videti globalno stanje otvorite naredni link:
                            </div>
                            <div class="col-sm-12">
                                <a href="/UserPages/HallOfFame" class="stretched-link text-primary hoverable"
                                    data-toggle="tooltip">Hall of fame</a>
                            </div>
                        </div>
                        <div class="rounded">
                            <div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <input style="visibility: hidden;" />
                                    </div>
                                </div>
                            </div>

                            <div class="container-fluid bg-white rounded mt-5">
                                <div class="col-sm-12">
                                    <br>
                                    <table id="tableK" class="table table-striped table-hover table-bordered">
                                        <thead id="tableKHead">

                                        </thead>
                                        <tbody id="tableKBody">

                                        </tbody>
                                    </table>
                                    <ul id="brojOsvojenihPoenaId">
                                    </ul>
                                    <ul id="datumiId">
                                    </ul>
                                    <ul id="predmetiId">
                                    </ul>
                                    @foreach(var item in Model.QuizData) {
                                    <script>
                                        var li = document.createElement("li");
                                        var ul = document.getElementById("brojOsvojenihPoenaId");
                                        li.innerHTML = @item.BrojOsvojenihPoena;
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                        li = document.createElement("li");
                                        ul = document.getElementById("datumiId");
                                        li.innerHTML = "@item.DatumZavrsetkaKviza";
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                        li = document.createElement("li");
                                        ul = document.getElementById("predmetiId");
                                        li.innerHTML = "@item.Predmet";
                                        li.style.display = "none";
                                        ul.appendChild(li);
                                    </script>
                                    }
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--Kalendar-->
                    <div id="div-kalendar" style="display: none;">
