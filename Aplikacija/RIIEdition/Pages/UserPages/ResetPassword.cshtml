@page
@model RIIEdition.Pages.UserPages.ResetPasswordModel
@{
}
<body>
  <partial name="_HeaderPartial" />
    <div class="container">
        <main role="main" >
          <h1>Reset Password</h1>
<div class="text-danger">@Model.Error</div>
<div asp-validation-summary="All" class="text-danger"></div>
<form method="post" >
    <div class="form-group col-sm-4" >
        <label asp-for="Email" ></label>
        <input asp-for="Email" class="form-control container-4" />
    </div>
    <div class="form-group col-sm-4" >
        <label asp-for="Password" ></label>
        <input asp-for="Password" class="form-control container-4" />
    </div>
    <div class="form-group col-sm-4" >
        <label asp-for="ConfirmPassword" ></label>
        <input asp-for="ConfirmPassword" class="form-control container-4" />
    </div>
    <button class="btn btn-primary" type="submit">Sumbit</button>


</form>



        </main>
    </div>
<partial name="_ScriptsPartial" />

</body>